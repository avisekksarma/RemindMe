{
    "manifest_version": 2,
    "name": "task-remainder",
    "version": "1.0",
    "description": "An extension to remind task in the set time",
    "icons": {
        "48": "icons/task-48.png"
    },
    "permissions": [
        "activeTab"
    ],
    "browser_action": {
        "default_icon": "icons/task-32.png",
        "default_title": "RemindMe",
        "default_popup": "popup/task_remainder.html"
    }
}